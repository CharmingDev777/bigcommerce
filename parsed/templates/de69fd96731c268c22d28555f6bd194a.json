                  </span>\n                    <span class=\"_less\">{{lang 'common.nav_less'}} \n                        <svg class=\"icon\"><use xlink:href=\"#icon-remove\"></use></svg>\n                    </span>\n                </a>\n                <a class=\"navPages-action has-subMenu\" href=\"{{urls.account.index}}\" data-collapsible=\"navPages-account\">\n                    {{lang 'common.account'}}\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                        </li>\n                        {{#if settings.returns_enabled}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action\" href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\n                        </li>\n                        {{#if settings.show_payment_methods}}\n                            <li class=\"navPage-subMenu-item\">\n                                <a class=\"navPage-subMenu-action\" href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\n                            </li>\n                        {{/if}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.mobile_nav.wishlists'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n                        </li>\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n                        </li>\n                    </ul>\n                </div>\n            </li>\n            <li class=\"navPages-item navPages-item--logout\">\n                <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            </li>\n        </ul>\n    {{/if}}\n\n    <ul class=\"navPages-list\">\n        {{#if theme_settings.navPages_type '==' 'standard'}}\n            {{#each categories}}\n                <li class=\"navPages-item navPages-item--standard\">\n                    {{> components/beautify/navigation-list theme_settings=../theme_settings menu_type='standard'}}\n                </li>\n            {{/each}}\n        {{else}}\n            {{#each categories}}\n                {{#if ../in_development}}\n                    {{#if name '==' 'Shop All'}}\n                        <li class=\"navPages-item navPages-item--standard\">\n                            {{> components/beautify/navigation-list theme_settings=../../../theme_settings menu_type='standard'}}\n                        </li>\n                    {{else}}\n                        <li class=\"navPages-item\">\n                            {{> components/beautify/navigation-list theme_settings=../../../theme_settings menu_type='mega'}}\n                        </li>\n                    {{/if}}\n                {{else}}\n                    <li class=\"navPages-item\">\n                        {{> components/beautify/navigation-list theme_settings=../../theme_settings menu_type='mega'}}\n                    </li>\n                {{/if}}\n            {{/each}}\n        {{/if}}\n    \n        \n         {{#unless theme_settings.hide_content_navigation}}\n            {{assignVar 'navWebPageCount' 0}}\n            {{#each pages}}\n                    {{#if (occurrences name 'Blog')}}\n                        <li class=\"navPages-item navPages-item--page\">\n                            {{> components/beautify/navigation-list theme_settings=../theme_settings menu_type='standard' page_type='page'}}\n                        </li>\n                   {{/if}}\n            {{/each}}\n        {{/unless}}\n    </ul>\n\n    <!-- <ul class=\"navPages-list navPages-list--user\">\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                    {{{lang 'common.currency' code=currency_selector.active_currency_code}}} <svg class=\"icon\"><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-bs-arrow\"></use></svg>\n                </a>\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                {{#if is_active}}\n                                    <strong>{{name}}</strong>\n                                {{else}}\n                                    {{name}}\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n            </li>\n        {{/if}}\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--storeCredit\">\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\n            <li class=\"navPages-item\">\n                <a class=\"navPages-action navPages-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\n            </li>\n        {{/if}}\n        <li class=\"navPages-item navPages-item--cart\">\n            <a\n                class=\"navPages-action navPages-action--cart\"\n                data-cart-preview\n                data-dropdown=\"cart-preview-dropdown\"\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\"\n                title=\"{{lang 'common.cart'}}\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-ih-bag-menu\" /></svg>\n                <span class=\"navPages-item-cartLabel\">{{lang 'common.cart'}}</span> <span class=\"countPill cart-quantity\"></span>\n            </a>\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul> -->\n</nav>\n","components/common/header":"{{#or banners.top_metadata theme_settings.instantload}}\n<div class=\"banners\" data-banner-location=\"top\">\n    {{#if settings.data_tag_enabled}}\n        {{#each banners.top_metadata}}\n            <div class=\"banner\">\n                {{{this.content}}}\n            </div>\n        {{/each}}\n    {{else}}\n        {{#each banners.top_metadata}}\n            <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\n                <div data-event-type=\"promotion-click\">\n                    {{{this.content}}}\n                </div>\n            </div>\n        {{/each}}\n    {{/if}}\n</div>\n{{/or}}\n<header class=\"header\" role=\"banner\" {{#if theme_settings.header_sticky}} data-sticky-header{{/if}}>\n\n    <div class=\"beautify__topHeader\">\n        <div class=\"container\">\n            {{#if theme_settings.topHeader-global-message}}\n                <div class=\"_announce\">\n                {{{region name=\"header_top_announcement--global\"}}}\n                {{#if in_development}}\n                {{> components/beautify/demo/region-header-top-announcement}}\n                {{/if}}\n                </div>\n            {{/if}}\n          \n        </div>\n    </div>\n    <div class=\"beautify__mainHeader{{#if settings.store_logo.image}} _hasImg{{/if}} _{{theme_settings.logo_position}}\">\n        <div class=\"main-header\">\n            {{#if template_file '===' 'pages/home'}}\n                <div class=\"header-logo logo-section{{#if settings.store_logo.image}} _isImg{{/if}}\">\n                    {{> components/common/store-logo}}\n                </div>\n            {{else}}\n                <div class=\"header-logo{{#if settings.store_logo.image}} _isImg{{/if}}\">\n                    {{> components/common/store-logo}}\n                </div>\n            {{/if}}\n          \n            <ul class=\"navUser-section user-section\">\n                <div class=\"wrapper-phone\"><a class=\"phone-number\" href=\"tel:8883616662\">888-361-6662</a>\n                    <div class=\"links-wrapper\">\n                    \t {{#if customer}}\n                        \t<a href=\"{{urls.account.index}}\">Orders</a>\n                         {{else}}\n                         \t<a href=\"{{urls.auth.login}}\">Orders</a>\n                         \t{{/if}}\n                        <a href=\"/contact-qrfs-by-phone-or-email/\">Contact Us</a>\n                    </div>\n                    </div>\n                {{#if settings.phone_number}}\n                <li class=\"navUser-item navUser-item--phone\">\n                    <a class=\"navUser-action\" href=\"tel:{{settings.phone_number}}\" title=\"{{lang 'header.call_us' phone_number=settings.phone_number}}\">\n                        <span class=\"_label\">{{lang 'header.call_us' phone_number=settings.phone_number}}</span>\n                    </a>\n                </li>\n                {{/if}}\n\n                <li class=\"navUser-item navUser-item--account\">\n                    {{#if customer}}\n                        <a class=\"navUser-action\" href=\"{{urls.account.index}}\">\n                            <img class=\"pro-icon\" src=\"https://cdn11.bigcommerce.com/s-eplfis85ct/images/stencil/original/image-manager/acnticon.png\" alt=\"Account\" title=\"Account\">\n                            <span class=\"_label\">{{lang 'common.account'}}</span>\n                        </a>\n                    {{else}}\n                        <a class=\"navUser-action\" href=\"{{urls.auth.login}}\">\n                          <img class=\"pro-icon\" src=\"https://cdn11.bigcommerce.com/s-eplfis85ct/images/stencil/original/image-manager/acnticon.png\" alt=\"Account\" title=\"Account\">\n                            <span class=\"_label\">\n                                {{lang 'common.login'}}\n                               \n                            </span>\n                        </a>\n                    {{/if}}\n                </li>\n            \n                {{#if customer.store_credit.value '>' 0}}\n                    <li class=\"navUser-item\">\n                        <a class=\"navUser-action navUser-action--storeCredit\"\n                            data-dropdown=\"storeCredit-dropdown\"\n                            data-options=\"align:bottom\"\n                            href=\"{{urls.cart}}\">\n                            <svg class=\"icon\"><use xlink:href=\"#icon-credit\" /></svg>\n                            <span class=\"_label\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\n                        </a>\n                        <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                            {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                        </div>\n                    </li>\n                {{/if}}\n            \n                <li class=\"navUser-item navUser-item--compare\">\n                    <a class=\"navUser-action navUser-action--compare\" href=\"{{urls.compare}}\" data-compare-nav>\n                        <svg class=\"icon\"><use xlink:href=\"#icon-compare\" /></svg>\n                        <span class=\"_label\">{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></span>\n                        \n                    </a>\n                </li>\n                \n                {{#if settings.gift_certificates_enabled}}\n                    <li class=\"navUser-item navUser-item--giftCert\">\n                        <a class=\"navUser-action navUser-action--giftCert\" href=\"{{urls.gift_certificate.purchase}}\">\n                            <svg class=\"icon\"><use xlink:href=\"#icon-gift\" /></svg>\n                            <span class=\"_label\">{{{lang 'common.gift_cert'}}}</span>\n                        <