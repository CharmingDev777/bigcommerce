}}\n                    <a class=\"navPages-action-toggle\" href=\"#\"\n                        data-collapsible=\"navPages-{{#if ../../page_type '==' 'page'}}page-{{getVar 'navWebPageCount'}}{{/if}}{{id}}{{../../idSuffix}}\">\n                        <span class=\"_more\">{{lang 'common.nav_more'}} <svg class=\"icon\"><use xlink:href=\"#icon-add\"></use></svg></span>\n                        <span class=\"_less\">{{lang 'common.nav_less'}} <svg class=\"icon\"><use xlink:href=\"#icon-remove\"></use></svg></span>\n                    </a>\n                    <a\n                        class=\"navPage-subMenu-action has-subMenu\"\n                        href=\"{{url}}\"\n                        {{#if ../../menu_type '!=' 'vertical'}}data-instantload{{/if}}\n                        {{#if id '==' ../../category.id}}data-current-category{{/if}}\n                    >{{name}}\n                    <!-- <i class=\"fa fa-angle-down\" aria-hidden=\"true\"></i> -->\n                </a>\n                    <!-- <a class=\"navPages-action-toggle-desktop\" href=\"#\" data-collapsible=\"navPages-{{#if ../../page_type '==' 'page'}}page-{{getVar 'navWebPageCount'}}{{/if}}{{id}}{{../../idSuffix}}\" title=\"{{lang 'common.nav_more'}}\"><svg class=\"icon\"><use xlink:href=\"#icon-caret-down\"></use></svg></a> -->\n                    <ul class=\"navPage-childList\" id=\"navPages-{{#if ../../page_type '==' 'page'}}page-{{getVar 'navWebPageCount'}}{{/if}}{{id}}{{../../idSuffix}}\">\n                        {{#each children}}\n                            <!-- {{incrementVar 'navWebPageCount'}} -->\n                            <li class=\"navPage-childList-item\">\n                                {{> components/beautify/navigation-list-child \n                                        category_id=../../../category.id \n                                        idSuffix=../../../idSuffix \n                                        theme_settings=../../../theme_settings\n                                        menu_type=../../../menu_type\n                                        page_type=../../../page_type}}\n                            </li>\n                        {{/each}}\n                        {{!--\n                        <!--\n                        {{#if ../../menu_type '==' 'mega'}}\n                            <li class=\"navPage-childList-item\">\n                                <a class=\"navPage-childList-action\" href=\"{{url}}\" data-instantload>{{lang 'common.view_all'}}</a>\n                            </li>\n                        {{/if}}\n                        -->\n                        --}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action\" href=\"{{url}}\" data-instantload {{#if id '==' ../../category.id}}data-current-category{{/if}}>{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action\" href=\"{{url}}\" data-instantload {{#if id '==' category.id}}data-current-category{{/if}}>{{name}}</a>\n{{/if}}\n","components/beautify/navigation-list-child":"{{#if children}}\n    <a class=\"navPages-action-toggle\" href=\"#\"\n        data-collapsible=\"navPages-{{#if page_type '==' 'page'}}page-{{getVar 'navWebPageCount'}}{{/if}}{{id}}{{idSuffix}}\">\n        <span class=\"_more\">{{lang 'common.nav_more'}} <svg class=\"icon\"><use xlink:href=\"#icon-add\"></use></svg></span>\n        <span class=\"_less\">{{lang 'common.nav_less'}} <svg class=\"icon\"><use xlink:href=\"#icon-remove\"></use></svg></span>\n    </a>\n    <a\n        class=\"navPage-childList-action has-subMenu\" \n        href=\"{{url}}\"\n        {{#if menu_type '!=' 'vertical'}}data-instantload{{/if}}\n        {{#if id '==' category_id}}data-current-category{{/if}}\n    >{{name}}</a>\n    <a class=\"navPages-action-toggle-desktop\" href=\"#\" data-collapsible=\"navPages-{{#if page_type '==' 'page'}}page-{{getVar 'navWebPageCount'}}