{"components/products/modals/fastShipping":"<div id=\"modal-shipping-description\" class=\"modal\" data-reveal>\n    <div class=\"modal-header\">\n        <h2 class=\"modal-header-title\">{{lang 'products.fast_shipping_and_easy_return'}}</h2>\n        <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n            <svg class=\"icon\"><use xlink:href=\"#icon-close\" /></svg>\n        </a>\n    </div>\n    <div class=\"modal-body\">\n        <div class=\"fast-shipping-desciption\"></div>  \n    </div>\n</div>\n<script>\n    var url=window.origin+\"/fast-shipping-and-easy-return/\"; \n    \n        if(window.origin!=undefined && window.origin!=\"\")\n        {   \n            $.get(url, function(result){\n                var content=\"\";\n                          if(result!=\"\")\n                          {\n                            var parser = new DOMParser(); \n                            var htmlDoc = parser. parseFromString(result, 'text/html');\n                            if(htmlDoc!=\"\")\n                                {\n                                    content=$(htmlDoc).find(\"body .page-content-body\").html();\n                                    if(content!=undefined && content!=\"\")\n                                    {\n                                        $(\".fast-shipping-desciption\").html(content);\n                                    }else{\n                                        content=$(htmlDoc).find(\"body\").html();\n                                        if(content!=undefined && content!=\"\"){\n                                             $(\".fast-shipping-desciption\").html(content);\n                                        }else{\n                                            $(\".fast-shipping-desciption\").html(\"<h2>Page not found! Please create a web page for shipping and easy return</h2>\"); \n                                        }  \n                                 }\n                                    \n                                }\n                          }else{\n                            $(\".fast-shipping-desciption\").html(\"<h2>Page not found! Please create a web page for shipping and easy return</h2>\");\n                          }\n                      \n              });\n        }           \n  </script>"}
